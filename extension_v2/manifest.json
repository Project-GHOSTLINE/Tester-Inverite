{
  "manifest_version": 3,
  "name": "Inverite Rapport Simple",
  "version": "9.1.3",
  "description": "Extension Inverite - 7 preteurs accordeons (645 noms)",
  "permissions": ["activeTab", "storage"],
  "host_permissions": [
    "*://inverite.com/*",
    "*://*.inverite.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://inverite.com/*",
        "*://*.inverite.com/*"
      ],
      "js": ["config.js", "content-script.js"],
      "run_at": "document_idle"
    }
  ]
}
